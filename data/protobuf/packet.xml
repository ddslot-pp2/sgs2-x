<?xml version="1.0" encoding="UTF-8"?>
<packet>

	<LOBBY start="1000">
		<!--게임 데이터 타입위한 enum값-->
		<GameDataType type="enum">
			 <test_1/>
			 <test_2/>
			 <test_3/>
		</GameDataType>

		<!-- 로그인 위한 패킷-->
		<CS_LOG_IN>
			<id type="string"/>
			<password type="string"/>
		</CS_LOG_IN>
		<SC_LOG_IN>
			<result type="bool"/>
			<timestamp type="int64"/>
			<nickname type="string"/>
			<medal_count type="int32"/>
			<coin_count type="int32"/>
		</SC_LOG_IN>

		<FIELD_INFO struct = "true">
			<field_id type="int32"/>
			<user_count type="int32"/>
			<max_user_count type="int32"/>
		</FIELD_INFO>

		<CS_FIELD_LIST>
		</CS_FIELD_LIST>
		<SC_FIELD_LIST>
			<result type="bool"/>
			<field_infos type="FIELD_INFO" repeated="true"/>
		</SC_FIELD_LIST>

		<CS_ENTER_FIELD>
			<field_id type="int32"/>
		</CS_ENTER_FIELD>
		<SC_ENTER_FIELD>
			<result type="bool"/>
			<field_id type="int32"/>
			<ec type="string"/>
		</SC_ENTER_FIELD>

		<CS_LEAVE_FIELD>
			<field_id type="int32"/>
		</CS_LEAVE_FIELD>
		<SC_LEAVE_FIELD>
			<result type="bool"/>
			<ec type="string"/>
		</SC_LEAVE_FIELD>

		<CS_PURCHASE_CHARACTER>
			<character_type type="int32"/>
		</CS_PURCHASE_CHARACTER>
		<SC_PURCHASE_CHARACTER>
			<medal_count type="int32"/>
			<coin_count type="int32"/>
			<result type="bool"/>
			<ec type="string"/>
		</SC_PURCHASE_CHARACTER>

	</LOBBY>

	<GAME start="2000">
		<CS_PING>
			<timestamp type="int64"/>
		</CS_PING>
		<SC_PING>
			<timestamp type="int64"/>
		</SC_PING>

		<!-- 패킷 아님; 유저 정보 구조체 -->
		<OTHER_INFO struct = "true">
			<obj_id type="int64"/>
			<tank_type type="int32"/>
			<nickname type="string"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
			<max_hp type="int32"/>
			<hp type="int32"/>
			<speed type="float"/>
		</OTHER_INFO>

		<CS_SYNC_FIELD>
			<field_id type="int32"/>
		</CS_SYNC_FIELD>
		<SC_SYNC_FIELD>
			<obj_id type="int64"/>
			<tank_type type="int32"/>
			<nickname type="string"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
			<max_hp type="int32"/>
			<hp type="int32"/>
			<speed type="float"/>
			<reload_time type="float"/>
			<other_infos type="OTHER_INFO" repeated="true"/>
		</SC_SYNC_FIELD>
		
		<SC_NOTI_OTHER_ENTER_FIELD>
			<obj_id type="int64"/>
			<tank_type type="int32"/>
			<nickname type="string"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
			<max_hp type="int32"/>
			<hp type="int32"/>
			<speed type="float"/>
		</SC_NOTI_OTHER_ENTER_FIELD>

		<SC_NOTI_OTHER_LEAVE_FIELD>
			<obj_id type="int64"/>
		</SC_NOTI_OTHER_LEAVE_FIELD>

		<CS_NOTI_MOVE>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
		</CS_NOTI_MOVE>

		<SC_NOTI_OTHER_MOVE>
			<obj_id type="int64"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
		</SC_NOTI_OTHER_MOVE>

		<BULLET_INFO struct = "true">
			<bullet_id type="int64"/>
			<dir_x type="float"/> <!-- bullet diretion -->
			<dir_y type="float"/>
			<dir_z type="float"/>
			<size_x type="float"/>
			<size_y type="float"/>
			<size_z type="float"/>
			<speed type="float"/>
			<distance type="float"/>
		</BULLET_INFO>

		<CS_FIRE>
			<bullet_type  type="int32"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
			<dir_x type="float"/> <!-- look diretion -->
			<dir_y type="float"/>
			<dir_z type="float"/>
			<bullet_infos type="BULLET_INFO" repeated="true"/>
		</CS_FIRE>

		<SC_NOTI_FIRE>
			<obj_id type="int64"/>
			<bullet_type type="int32"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
			<dir_x type="float"/> <!-- look diretion -->
			<dir_y type="float"/>
			<dir_z type="float"/>
			<bullet_infos type="BULLET_INFO" repeated="true"/>
		</SC_NOTI_FIRE>

		<DAMAGE_INFO struct = "true">
			<target_id type="int64"/>
			<hp type="int32"/>
			<damage type="int64"/>
		</DAMAGE_INFO>

		<SC_NOTI_DESTROY_BULLET>
			<obj_id type="int64"/>
			<bullet_id type="int64"/>
			<damage_infos type="DAMAGE_INFO" repeated="true"/>
		</SC_NOTI_DESTROY_BULLET>

		<SC_NOTI_DESTROY_CHARACTER>
			<obj_id type="int64"/>
		</SC_NOTI_DESTROY_CHARACTER>

		<CS_RESPAWN_CHARACTER>

		</CS_RESPAWN_CHARACTER>

		<SC_NOTI_RESPAWN_CHARACTER>
			<obj_id type="int64"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
			<max_hp type="int32"/>
			<hp type="int32"/>
			<speed type="float"/>
			<reload_time type="float"/>
		</SC_NOTI_RESPAWN_CHARACTER>

		<SC_SELECT_BUFF>
			<count type="int32"/>
		</SC_SELECT_BUFF>

		<CS_ENHANCE_BUFF>
			<buff_type type="int32"/>
		</CS_ENHANCE_BUFF>

		<SC_NOTI_UPDATE_CHARACTER_STATUS>
			<obj_id type="int64"/>
			<max_hp type="int32"/>
			<hp type="int32"/>
			<speed type="float"/>
			<reload_time type="float"/>
		</SC_NOTI_UPDATE_CHARACTER_STATUS>

		<ITEM_INFO struct = "true">
			<item_id type="int64"/>
			<item_type type="int32"/>
			<pos_x type="float"/>
			<pos_y type="float"/>
			<pos_z type="float"/>
		</ITEM_INFO>

		<SC_NOTI_ACTIVE_ITEM>
			<item_infos type="ITEM_INFO" repeated="true"/>
		</SC_NOTI_ACTIVE_ITEM>

		<SC_NOTI_ACQUIRE_ITEM>
			<obj_id type="int64"/>
			<item_id type="int64"/>
			<item_type type="int32"/>
			<hp type="int32"/>
			<shield_time type="int32"/>
			<exp type="int32"/>
		</SC_NOTI_ACQUIRE_ITEM>

		<MEDAL_ITEM_INFO struct = "true">
			<item_id type="int64"/>
			<item_type type="int32"/>
			<from_pos_x type="float"/>
			<from_pos_y type="float"/>
			<from_pos_z type="float"/>
			<to_pos_x type="float"/>
			<to_pos_y type="float"/>
			<to_pos_z type="float"/>
		</MEDAL_ITEM_INFO>

		<SC_NOTI_CREATE_MEDAL_ITEM>
			<medal_item_infos type="MEDAL_ITEM_INFO" repeated="true"/>
		</SC_NOTI_CREATE_MEDAL_ITEM>

		<SC_NOTI_ACQUIRE_PERSIST_ITEM>
			<obj_id type="int64"/>
			<item_id type="int64"/>
			<item_type type="int32"/>
			<count type="int32"/>
		</SC_NOTI_ACQUIRE_PERSIST_ITEM>

		<RANK_INFO struct = "true">
			<object_id type="int64"/>
			<nickname type="string"/>
			<score type="int32"/>
		</RANK_INFO>

		<SC_NOTI_RANK_INFO>
			<rank_infos type="RANK_INFO" repeated="true"/>
		</SC_NOTI_RANK_INFO>

		<SC_NOTI_RANK>
			<rank type="int32"/>
			<nickname type="string"/>
			<score type="int32"/>
		</SC_NOTI_RANK>

	</GAME>


</packet>
